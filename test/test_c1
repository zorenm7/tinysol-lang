// unit testing c1.sol
faucet 0xA 100
deploy 0xA:0xC1() "test/c1.sol"
faucet 0xC1 10
assert 0xC1 balance 10
0xA:0xC1.f1()
assert 0xC1 x 0
assert 0xC1 b true
assert 0xA balance 100
0xA:0xC1.f1{value : 1}()
assert 0xC1 x 1
assert 0xC1 b true
assert 0xA balance 99
assert 0xC1 balance 11
0xA:0xC1.f2("0xA")
assert 0xC1 x 1
assert 0xC1 b true
assert 0xC1 balance 10
assert 0xA balance 100
0xA:0xC1.f3(3)
assert 0xC1 x 1
assert 0xC1 b true
assert 0xC1 balance 7
assert 0xA balance 103
0xA:0xC1.f3(3)
assert 0xC1 x 1
assert 0xC1 b false
assert 0xC1 balance 7
assert 0xA balance 103